{
    "editor.renderWhitespace": "all",
    "editor.tabSize": 2,
    "editor.lineHeight": 18,
    "editor.fontFamily": "'Fira Code'",
    "editor.fontWeight": "400",
    "workbench.colorCustomizations": {
      "[One Monokai]": {
        // // // Main color
        "statusBar.background": "#181818",
        "panel.background": "#181818",
        "editor.background": "#232323",
        "tab.activeBackground": "#232323",
        "titleBar.activeBackground": "#181818",
        "sideBarSectionHeader.background": "#181818",
        "list.hoverBackground": "#181818",
  
        // // // Second color
        "sideBar.background": "#292929",
        "activityBar.background": "#181818",
        "tab.inactiveBackground": "#181818",
        "input.background": "#292929",
        "editor.lineHighlightBackground": "#292929",
        "tab.hoverBackground": "#292929",
        "menu.background": "#181818",
        "titleBar.activeForeground": "#fff",
        "titleBar.inactiveBackground": "#181818",

        // Other colors
        "gitDecoration.modifiedResourceForeground": "#ffeb3b",
        "gitDecoration.untrackedResourceForeground": "#a200ff",
        "gitDecoration.addedResourceForeground": "#77df00"
      }
    },
    "workbench.startupEditor": "none",
    "window.zoomLevel": 0,
    "editor.smoothScrolling": true,
    "editor.minimap.showSlider": "always",
    "editor.minimap.renderCharacters": false,
    "editor.formatOnType": false,
    "editor.cursorBlinking": "expand",
    "editor.renderLineHighlight": "all",
    "window.restoreFullscreen": true,
    "window.title": "${rootName}",
    "editor.fontLigatures": true,
    "files.defaultLanguage": "javascript",
    "explorer.openEditors.visible": 0,
    "editor.rulers": [
      100
    ],
    "terminal.integrated.fontFamily": "'Fira Code'",
    "terminal.integrated.fontWeight": "400",
    "explorer.confirmDragAndDrop": false,
    "explorer.confirmDelete": false,
    "html.format.wrapLineLength": 0,
    "javascript.validate.enable": false,
    "editor.suggestSelection": "recentlyUsedByPrefix",
    "editor.minimap.enabled": true,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "files.associations": {
      "Dockerfile*": "dockerfile"
    },
    "git.confirmSync": false,
    "workbench.colorTheme": "One Monokai",
    "workbench.editor.labelFormat": "short",
    "breadcrumbs.enabled": true,
    "typescript.updateImportsOnFileMove.enabled": "always",
    "workbench.iconTheme": "vscode-icons",
    "javascript.implicitProjectConfig.checkJs": true,
    "gitlab.instanceUrl": "https://gitlab.fintonic.com",
    "gitlab.showPipelineUpdateNotifications": true,
    "gitlab.enableExperimentalFeatures": true,
    "terminal.integrated.fontSize": 11,
    "terminal.integrated.fontWeightBold": "600",
  }
